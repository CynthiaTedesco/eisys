<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
  <% include ../partials/page-header.ejs %>
  <% include ../partials/nav.ejs %>

<div class="slideshow jumbotron text-center">
    <section id="slideshow" class="header-line"><!--slideshow-->
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
          <li data-target="#myCarousel" data-slide-to="3"></li>
          <li data-target="#myCarousel" data-slide-to="4"></li>
          <li data-target="#myCarousel" data-slide-to="5"></li>
        </ol>
        <div id="carousel-inner-lexar" class="carousel-inner" role="listbox">
          <div class="item active">
            <img class="first-slide img-responsive" src="images/slideshow/slide01.jpg" alt="First slide">
            <div class="container">
              <div class="carousel-caption">
                <p class="">
                  <span><%= slideshow_caption %></span>
                </p>
              </div>
            </div>
          </div>
          <div class="item">
            <img class="second-slide" src="images/slideshow/slide02.jpg" alt="Second slide">
            <div class="container">
              <div class="carousel-caption">
                <p class="">
                  <span><%= slideshow_caption %></span>
              </div>
            </div>
          </div>
          <div class="item">
            <img class="third-slide" src="images/slideshow/slide03.jpg" alt="Third slide">
            <div class="container">
              <div class="carousel-caption">
                <p class="">
                  <span><%= slideshow_caption %></span>
                </p>
              </div>
            </div>
          </div>
        </div>
        <a class="left carousel-control hidden-xs hidden-sm" href="#myCarousel" role="button" data-slide="prev">
          <img class="v-center left" src="images/slideshow/arrow-left-off.png" aria-hidden="true"></img>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control hidden-xs hidden-sm" href="#myCarousel" role="button" data-slide="next" style="align:center">
          <img class="v-center right" src="images/slideshow/arrow-right-off.png" aria-hidden="true"></img>
          <span class="sr-only">Next</span>
        </a>
      </div><!-- /.carousel -->
    </section><!--fin del slideshow-->
</div>
<div class="container">
  <div class="row">
    <div class="col-sm-7">
      <div class="text-center" role="alert">
        compromiso
      </div>
      <hr class="visible-xs">
      <div class="text-center" role="alert">
        alianzas
      </div>
      
    </div>
    <div class="text-center col-sm-5">
      otros links
    </div>
   </div> <!--row-->
   <div class="text-center">servicios</div>
   <div class="text-center">productos</div>
</div>
<% include ../partials/page-footer.ejs %>
</body>
</html>
